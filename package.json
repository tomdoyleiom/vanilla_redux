{
  "name": "vanilla_redux_nrw_testing",
  "version": "0.0.1",
  "description": "a test vanilla javascript application using redux for discovery into a future nrw project",
  "main": "src/main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "webpack --config webpack.config.js",
    "build:watch": "webpack --config webpack.config.js --watch",
    "api": "json-server --watch db.json",
    "dev": "concurrently --kill-others \"npm run build:watch\" \"npm run api\" \"serve ./build\""
  },
  "keywords": [
    "redux",
    "js"
  ],
  "author": "t doyle",
  "license": "ISC",
  "dependencies": {
    "dotenv-webpack": "^1.7.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serve": "^11.3.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "jest": "^24.9.0",
    "json-server": "^0.15.1",
    "redux-testkit": "^1.0.6",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
